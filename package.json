{
  "name": "@rajmasha/tic-tac-toe",
  "version": "0.1.4",
  "description": "tic-tac-toe is a TypeScript library for creating an instance of the game Tic Tac Toe with useful methods to make a move, undo a move, check game status, check if there is a winner, reset the game and much more.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch index.ts",
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "test": "bun test",
    "lint": "tsc",
    "release": "changeset publish"
  },
  "keywords": [
    "tic tac toe",
    "game",
    "typescript"
  ],
  "author": "Raj Asha",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "bun-types": "^1.0.6",
    "tsup": "^7.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "src/index.ts"
  ]
}
